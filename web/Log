            ON t1.parent = t2.id

SELECT  t1.id,t1.parent, t1.name as leaf_name, t2.name as node_name, t2.id as parent FROM types as t1
            INNER JOIN types as t2 
            ON t1.parent = t2.id WHERE t2.name = 'Основні'

